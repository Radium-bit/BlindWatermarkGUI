## 【注意】 此文件会覆盖DEV.ENV的同名内容，敬请留意

#构建时输出文件名是否包括 Git Hash 
INCLUDE_GIT_HASH=true
# 程序构建版本号
BUILD_VERSION='0.2.4'
# 是否启用控制台调试
ENABLE_CONSOLE_DEBUG=false
# 是否启用优化模式（0：无优化调试用，1：基础优化，2：深度优化）
OPTIMIZE=2
# 是否启用压缩
ENABLE_COMPRESS=true
# 压缩等级
# 0-9，越高压缩率越高，但压缩时间也越长
COMPRESS_LEVEL=6
# 是否额外打包Protable格式
INCLUDE_PROTABLE=true
# 是否额外打包安装程序格式
INCLUDE_INSTALLER=true
# 程序GUID，用于安装后识别
PROGRAM_GUID='375ed30c-67dd-4a92-aae0-2f3e6e4da8dd'